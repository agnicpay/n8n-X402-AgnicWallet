{
  "name": "n8n-nodes-agnicwallet",
  "version": "1.0.14",
  "description": "n8n community node for AgnicWallet - automated Web3 payments for X402 APIs",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "agnicwallet",
    "x402",
    "web3",
    "crypto",
    "payments",
    "blockchain",
    "usdc",
    "automation",
    "workflow",
    "base",
    "solana",
    "mcp",
    "model-context-protocol",
    "ai-agent"
  ],
  "license": "MIT",
  "homepage": "https://github.com/agnicpay/n8n-X402-AgnicWallet#readme",
  "author": {
    "name": "AgnicWallet",
    "email": "support@agnicpay.xyz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agnicpay/n8n-X402-AgnicWallet.git"
  },
  "bugs": {
    "url": "https://github.com/agnicpay/n8n-X402-AgnicWallet/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "node esbuild.config.js && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint nodes credentials --ext .ts",
    "lint:fix": "eslint nodes credentials --ext .ts --fix",
    "format": "prettier nodes credentials --write",
    "format:check": "prettier nodes credentials --check",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AgnicWalletApi.credentials.js",
      "dist/credentials/AgnicWalletOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/X402HttpRequest/X402HttpRequest.node.js",
      "dist/nodes/AgnicAILanguageModel/AgnicAILanguageModel.node.js",
      "dist/nodes/AgnicAI/AgnicAI.node.js",
      "dist/nodes/AgnicMCPTool/AgnicMCPTool.node.js"
    ]
  },
  "devDependencies": {
    "@langchain/core": "^0.3.68",
    "@langchain/openai": "^0.6.16",
    "@modelcontextprotocol/sdk": "^1.24.0",
    "@n8n/json-schema-to-zod": "^1.6.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^6.13.0",
    "esbuild": "^0.27.2",
    "eslint": "^8.54.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "glob": "^13.0.0",
    "gulp": "^4.0.2",
    "langchain": "^0.3.33",
    "n8n-workflow": "^1.40.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "zod": "^3.23.0"
  },
  "peerDependencies": {
    "@langchain/core": ">=0.2.0",
    "@n8n/json-schema-to-zod": "*",
    "n8n-workflow": "*"
  }
}
